{
	"name": "Tundra",
	"type": "dark",
	"$schema": "vscode://schemas/color-theme",
	"colors": {
		"editor.background": "#272822",
		"editor.foreground": "#f8f8f2",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb",

		"statusBar.background": "#00014b",
		"statusBar.foreground": "#f8f8f2",
		"statusBar.border"    : "#000000",
		"statusBar.debuggingBackground": "#000266",
		"statusBar.debuggingForeground": "#f8f8f2",
	},
	"tokenColors": [{
		"name": "Comments",
		"settings": {"foreground": "#00db00"},
		"scope": [
			"comment"
		]
	},{
		"name": "Operators",
		"settings": {"foreground": "#f49ac2"},
		"scope": [
			"keyword.operator",
			"punctuation.separator.key-value"
		]
	},{
		"name": "Single and double quotes",
		"settings": {"foreground": "#e6dc64"},
		"scope": [
			"punctuation.definition.string"
		]
	},{
		"name": "JSON Keys",
		"settings": {"foreground": "#d392ff"},
		"scope": [
			"variable.object.property.ts",
			"meta.object-literal.key",
			"meta.object-literal.key string.quoted.double",
			"meta.object-literal.key.js entity.name.function.js",
			"support.type.property-name"
		]
	},
	{
		"name": "Templates",
		"settings": {"foreground": "#ffb347"},
		"scope": [
			"punctuation.definition.string.template",
			"string.template"
		]
	},{
		"name": "({[",
		"settings": {"foreground": "#ffb347"},
		"scope": [
			"meta.brace.round",
			"meta.brace.square",
			"punctuation.definition.block",
			"punctuation.definition.parameters",
			"punctuation.definition.dictionary",
			"punctuation.definition.array",
			"punctuation.definition.binding-pattern.array"
		]
	},{
		"name": "Variables",
		"settings": {"foreground": "#ff6961"},
		"scope": [
			"variable.other.object",
			"variable.other.readwrite",
			"variable.other.property",
			"variable.other.constant",
			"variable.parameter",
			"support.variable.property"
		]
	},{
		"name": "Support Functions",
		"settings": {"foreground": "#e0e0e0"},
		"scope": [
			"support.function",
			"support.class",
			"support.variable.object.process"
		]
	},{
		"name": "Strings",
		"settings": {"foreground": "#e6dc64"},
		"scope": [
			"string.quoted.double"
		]
	},{
		"name": "Semicolon",
		"settings": {"foreground": "#e0e0e0"},
		"scope": [
			"punctuation.terminator.statement"
		]
	},{
		"name": "Commas",
		"settings": {"foreground": "#e0e0e0"},
		"scope": [
			"punctuation.separator.comma",
			"punctuation.separator.array"
		]
	},{
		"name": "Function Entity",
		"settings": {"foreground": "#77dd77"},
		"scope": [
			"entity.name.function"
		]
	},{
		"name": "Integers",
		"settings": {"foreground": "#ae81ff"},
		"scope": [
			"constant.numeric.decimal"
		]
	},{
		"name": "If Else For While Do Try Catch Return Var Function Async Await",
		"settings": {"foreground": "#87ceeb"},
		"scope": [
			"variable.parameter.ts",
			"keyword.control.from.ts",
			"keyword.control.import.ts",
			"keyword.control.loop",
			"keyword.control.conditional",
			"keyword.control.trycatch",
			"keyword.control.flow",
			"keyword.control.export",
			"support.type.builtin.ts",
			"keyword.operator.type.ts",
			"keyword.control.default",
			"storage.type.function",
			"storage.type",
			"storage.modifier.ts",
			"meta.function"
		]
	},{
		"name": "This",
		"settings": {"foreground": "#ffd700"},
		"scope": [
			"variable.language.this"
		]
	},{
		"name": "Accessor",
		"settings": {"foreground": "#77dd77"},
		"scope": [
			"punctuation.accessor"
		]
	},{
		"name": "True & False",
		"settings": {"foreground": "#c23b22"},
		"scope": [
			"constant.language.boolean"
		]
	},
	// HTML
	{
		"name": "HTML: <>",
		"settings": {"foreground": "#dbdbdb"},
		"scope": [
			"punctuation.definition.tag.begin.html",
			"punctuation.definition.tag.end.html"
		]
	},{
		"name": "HTML: <>",
		"settings": {"foreground": "#ff5c5c"},
		"scope": [
			"support.class.component.html"
		]
	},{
		"name": "HTML: Tag Names",
		"settings": {"foreground": "#ff3b9d"},
		"scope": [
			"entity.other.inherited-class.ts",
			"entity.name.type.interface.ts",
			"entity.name.tag.block.any.html",
			"entity.name.tag.template.html",
			"entity.name.tag.script.html",
			"entity.name.tag.style.html",
			"entity.name.tag.html"
		]
	},{
		"name": "HTML: Attribute Names",
		"settings": {"foreground": "#49e012"},
		"scope": [
			"entity.other.attribute-name.html"
		]
	},{
		"name": "HTML: =",
		"settings": {"foreground": "#f49ac2"},
		"scope": [
			"meta.tag.block.any.html",
			"meta.tag.other.html",
			"meta.tag.any.html"
		]
	},{
		"name": "HTML: Quoted Strings",
		"settings": {"foreground": "#e6dc64"},
		"scope": [
			"string.quoted.double.html",
			"text.html variable.other.readwrite.js"
		]
	},{
		"name": "{{ }}",
		"settings": {"foreground": "#7698e2"},
		"scope": [
			"punctuation.definition.generic.begin.html",
			"punctuation.definition.generic.end.html"
		]
	},{
		"name": "Text",
		"settings": {"foreground": "#d1d1ce"},
		"scope": [
			"text.html"
		]
	},
	// CSS
	{
		"name": "CSS: {}",
		"settings": {"foreground": "#ffb347"},
		"scope": [
			"punctuation.section.property-list.begin.bracket.curly.scss",
			"punctuation.section.property-list.end.bracket.curly.scss"
		]
	},{
		"name": "CSS: &",
		"settings": {"foreground": "#f49ac2"},
		"scope": [
			"entity.name.tag.reference.scss"
		]
	},{
		"name": "CSS: Pseudo-Elements",
		"settings": {"foreground": "#dab143"},
		"scope": [
			"entity.other.attribute-name.pseudo-class.css"
		]
	},{
		"name": "CSS: Tag Name",
		"settings": {"foreground": "#f92672"},
		"scope": [
			"entity.name.tag.css"
		]
	},{
		"name": "CSS: Attribute Name",
		"settings": {"foreground": "#49e012"},
		"scope": [
			"entity.other.attribute-name.class.css"
		]
	},{
		"name": "CSS: Property Name",
		"settings": {"foreground": "#ff6961"},
		"scope": [
			"support.type.property-name.css",
			"support.type.vendored.property-name.css"
		]
	},{
		"name": "CSS: Property Value (string)",
		"settings": {"foreground": "#87ceeb"},
		"scope": [
			"support.constant.property-value.css",
			"support.function.misc.scss",
			"punctuation.section.function.scss"
		]
	},{
		"name": "CSS: Property Value (numeric)",
		"settings": {"foreground": "#ae81ff"},
		"scope": [
			"constant.numeric.css"
		]
	},{
		"name": "CSS: Colors",
		"settings": {"foreground": "#daa520"},
		"scope": [
			"constant.other.color.rgb-value.hex.css",
			"support.constant.color.w3c-standard-color-name.css"
		]
	},{
		"name": "CSS: Parameter for URL",
		"settings": {"foreground": "#7ef37e"},
		"scope": [
			"variable.parameter.url.scss"
		]
	},{
		"name": "CSS: Semicolon",
		"settings": {"foreground": "#e0e0e0"},
		"scope": [
			"punctuation.terminator.rule.scss"
		]
	}]
}

// Red         | ff6961
// Orange      | ffb347
// Yellow      | e6dc64
// Green       | 77dd77
// Blue        | 87ceeb
// Purple      | ae81ff
// Gray        | cfcfc4
// Brown       | 836953
// Pink        | f49ac2
// Dark Red    | c23b22
// Dark Blue   | 779ecb
// Dark Purple | 966fd6
// Dark Green  | 03c03c
// Gold        | ffd700
// Goldenrod   | daa520
